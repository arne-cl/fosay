.past-verb-forms-1
:: pa-sm { sufix: "л"  }
:: pa-sf { sufix: "ла" }
:: pa-sl { sufix: "ло" }
:: pa-p  { sufix: "ли" }

.present-verb-forms-1
:: pr-s1  { sufix: "ю"   }
:: pr-p1  { sufix: "ем"  }
:: pr-s2i { sufix: "ешь" }
:: pr-s2f { sufix: "ете" }
:: pr-p2i { sufix: "ете" }
:: pr-s3  { sufix: "ет"  }
:: pr-p3  { sufix: "ют"  }

.conditional-verb-forms-1
:: co-sm { sufix: "л бы" }
:: co-sf { sufix: "ла б" }
:: co-sl { sufix: "ло б" }
:: co-p  { sufix: "ли б" }

.present-verb-forms-2
:: pr-s1  { sufix: "жу"  }
:: pr-p1  { sufix: "им"  }
:: pr-s2i { sufix: "ишь" }
:: pr-s2f { sufix: "ите" }
:: pr-p2i { sufix: "ите" }
:: pr-s3  { sufix: "ит"  }
:: pr-p3  { sufix: "ят"  }

.past-verb-forms-jela
:: pa-sm { sufix: "ел"  }
:: pa-sf { sufix: "ела" }
:: pa-sl { sufix: "ело" }
:: pa-p  { sufix: "ели" }

.conditional-verb-forms-jela
:: co-sm { sufix: "ел бы" }
:: co-sf { sufix: "ела б" }
:: co-sl { sufix: "ело б" }
:: co-p  { sufix: "ели б" }


.present-verb-forms-3
:: pr-s1  { sufix: "ю"   }
:: pr-p1  { sufix: "имо" }
:: pr-s2i { sufix: "иш"  }
:: pr-s2f { sufix: "ите" }
:: pr-p2i { sufix: "ите" }
:: pr-s3  { sufix: "ить" }
:: pr-p3  { sufix: "ять" }


.infinitive-jetj
:: inf { sufix: "еть" }

.infinitive-tj
:: inf { sufix: "ть" }

.verb-forms-3 ::
    infinitive-jetj
    past-verb-forms-jela
    past-verb-forms-jela(verb-perfect-present-u)
    present-verb-forms-3
    present-verb-forms-3(verb-perfect-future-u)
    conditional-verb-forms-jela
{
    type: verb;
}

.verb-forms-1 ::
    infinitive-tj
    past-verb-forms-1
    past-verb-forms-1(verb-perfect-present-u)
    present-verb-forms-1
    present-verb-forms-1(verb-perfect-future-u)
    conditional-verb-forms-1
{
    type: verb;
}


.verbs-2 ::
    infinitive-jetj
    past-verb-forms-jela
    past-verb-forms-jela(verb-perfect-present-u)
    present-verb-forms-2
    present-verb-forms-2(verb-perfect-future-u)
    conditional-verb-forms-jela
{
    type: verb;
}

.cont-verbs-2 ::
    infinitive-jetj
    past-verb-forms-jela
    past-verb-forms-jela(verb-perfect-present-u)
    past-verb-forms-jela(verb-perfect-continuous-not-durative)
    present-verb-forms-2
    present-verb-forms-2(verb-perfect-future-u)
    present-verb-forms-2(verb-continuous)
    present-verb-forms-2(verb-perfect-continuous-durative)
    conditional-verb-forms-jela
{
    type: verb;
}